{
  "name": "@commercialevs/charger-insights",
  "version": "1.1.0",
  "description": "A reusable EV charger finder component for commercial EV platforms",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css",
    "./theme.css": "./dist/theme.css"
  },
  "sideEffects": [
    "styles.css",
    "theme.css"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --config demo/vite.config.ts",
    "build": "tsup src/index.ts --dts --format esm,cjs --sourcemap",
    "postbuild": "node -e \"const fs=require('fs'); fs.copyFileSync('src/styles.css', 'dist/styles.css'); fs.copyFileSync('src/theme.css', 'dist/theme.css');\"",
    "build:demo": "npm run build && cd demo && npm install && npm run copy-api-key && npm run build",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "validate": "npm run lint && npm run type-check && npm run test",
    "prepublishOnly": "npm run validate && npm run build"
  },
  "keywords": [
    "ev",
    "electric-vehicle",
    "charger",
    "charging-station",
    "nrel",
    "react",
    "typescript",
    "leaflet",
    "commercial"
  ],
  "author": "Commercial EVs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercialevs/charger-insights.git"
  },
  "bugs": {
    "url": "https://github.com/commercialevs/charger-insights/issues"
  },
  "homepage": "https://github.com/commercialevs/charger-insights#readme",
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "leaflet": "^1.9.4",
    "react-leaflet": "^4.2.1",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.6.7",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/react-vite": "^7.6.7",
    "@storybook/testing-library": "^0.2.2",
    "@tanstack/react-query": "^5.8.4",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/leaflet": "^1.9.8",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.6",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
